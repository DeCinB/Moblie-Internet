<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homework2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Exercise1Activity"
                  android:label="作业1的Activity">
                  <intent-filter>
                      <action android:name="exercise1"></action>
                      <category android:name="my_category"/>
                      <category android:name="android.intent.category.DEFAULT"/>

                  </intent-filter>
        </activity>
        <activity android:name=".Exercise2Activity"
            android:label="作业2的Activity">
            <intent-filter>
                <action android:name="exercise2"></action>
                <category android:name="my_category"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>
        <activity android:name=".Exercise3Activity"
            android:label="作业3的Activity">
            <intent-filter>
                <action android:name="exercise3"></action>
                <category android:name="my_category"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>
        <activity android:name=".ChartroomActivity"/>

    </application>

</manifest>